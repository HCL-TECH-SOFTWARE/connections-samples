{
    "name": "My WebMeeting",
    "title": "My WebMeeting",
    "description": "Allows each user to quickly access their own Web Meeting",
    "services": [
        "Customizer"
    ],
    "state": "enabled",
    "extensions": [
        {
            "name": "webMeeting",
            "type": "com.ibm.customizer.ui",
            "payload": {
                "include-files": [
                    "webMeeting/commonTools.js",
                    "webMeeting/webMeeting.js"
                ],
                "cache-headers": {
                    "cache-control": "max-age=43200"
                }
            },
            "path": "global",
            "state": "enabled"
        }
    ]
}